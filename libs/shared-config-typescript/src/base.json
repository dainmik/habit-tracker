{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@tsconfig/strictest/tsconfig.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"incremental": false,
		"lib": ["es2024"],
		"module": "nodenext",
		"moduleDetection": "force",
		"moduleResolution": "nodenext",
		"noPropertyAccessFromIndexSignature": false,
		"resolveJsonModule": true,
		"target": "es2024"
	}
}
